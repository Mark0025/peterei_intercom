{
  "meta": {
    "title": "7-Levels Deep Onboarding Discovery",
    "description": "EOS-style questioning to understand why our onboarding works or fails",
    "targetAudience": ["Jon (Owner)", "Mark (Developer)"],
    "resolutionCategories": ["Education", "Coding", "Expectations", "Process", "Data Validation"]
  },
  "sections": [
    {
      "id": "data-upload",
      "title": "Data Upload & Migration",
      "shortDescription": "Understanding the data onboarding process",
      "questions": [
        {
          "id": "data-upload-l1",
          "level": 1,
          "question": "What is our current data upload process for new clients?",
          "context": "Start with the observable process",
          "expectedAnswerType": "process-description"
        },
        {
          "id": "data-upload-l2",
          "level": 2,
          "question": "Why do we use this specific data upload method?",
          "context": "Understand the reasoning behind the process",
          "expectedAnswerType": "rationale"
        },
        {
          "id": "data-upload-l3",
          "level": 3,
          "question": "Why is this method better than alternatives (manual entry, API sync, bulk imports)?",
          "context": "Challenge assumptions, explore tradeoffs",
          "expectedAnswerType": "comparative-analysis"
        },
        {
          "id": "data-upload-l4",
          "level": 4,
          "question": "Why do clients struggle with this process? What breaks down?",
          "context": "Identify pain points and failure modes",
          "expectedAnswerType": "problem-identification"
        },
        {
          "id": "data-upload-l5",
          "level": 5,
          "question": "Why haven't we fixed these issues yet? What's blocking us?",
          "context": "Uncover root causes and constraints",
          "expectedAnswerType": "constraint-analysis"
        },
        {
          "id": "data-upload-l6",
          "level": 6,
          "question": "Why is solving this problem important for business success?",
          "context": "Connect to business value and outcomes",
          "expectedAnswerType": "business-impact"
        },
        {
          "id": "data-upload-l7",
          "level": 7,
          "question": "Why would a perfect data upload process transform our client relationships?",
          "context": "Envision the ultimate outcome and impact",
          "expectedAnswerType": "vision-statement"
        }
      ]
    },
